#include<stdio.h>
	
main(){
		float salmini, salfunc, quantsal;
		printf("Insira o valor do salario minimo: ");
		scanf("%f",&salmini);
		printf("Insira o valor do salario do funcionario: ");
		scanf("%f",&salfunc);
	
		quantsal = (salfunc/salmini);
		
		printf("A quantidade de salarios minimos e %f",quantsal);
}
