#include<stdio.h>
#include<stdlib.h>	
int main(){
        int n1, n2, n3, n4, n5, num;
        printf("Digite um numero de 5 digitos para verificar se e PALINDROMO \n");
        scanf("%d",&num);
        
        n1 = num/1000;
        n2 = (num/1000)%10;
        n3 = (num%100)/10;
        n4 = num%10;
        
        if(n1==n5&&n2==n4){
           printf("O numero %d e PALINDROMO",num);
        }
        else{
             printf("Nao e PALINDROMO");
        }
		return 0;
}
